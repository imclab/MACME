(function(){tinymce.PluginManager.requireLangPack('macme');tinymce.create('tinymce.plugins.MacmePlugin',{init:function(ed,url){ed.addCommand('mceMacme',function(){ed.windowManager.open({file:url+'/dialog.htm',width:320+parseInt(ed.getLang('macme.delta_width',0)),height:120+parseInt(ed.getLang('macme.delta_height',0)),inline:1},{plugin_url:url,some_custom_arg:'custom arg'})});ed.addButton('macme',{title:'example.desc',cmd:'mceMacme',image:url+'/img/macme.gif'});ed.onNodeChange.add(function(ed,cm,n){cm.setActive('macme',n.nodeName=='IMG')})},createControl:function(n,cm){return null},getInfo:function(){return{longname:'Macme plugin',author:'Salvatore Iaconesi',authorurl:'http://www.artisopensource.net',infourl:'http://www.fakepress.it/macme',version:"0.1"}}});tinymce.PluginManager.add('macme',tinymce.plugins.MacmePlugin)})();